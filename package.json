{
  "name": "@nest-template/apis",
  "version": "0.0.1",
  "description": "Nest.js - API - Template",
  "main": "dist/main",
  "repository": "git@github.com:hieudoanm/nest-api-template.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GNU GPL 3.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist && prisma generate",
    "build": "nest build",
    "postbuild": "yarn docs:postman && yarn docs:html",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "kill-port 3000 && nest start",
    "start:dev": "kill-port 3000 && nest start --watch",
    "start:debug": "kill-port 3000 && nest start --debug --watch",
    "start:prod": "node dist/main",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:html": "redocly build-docs ./docs/swagger/swagger.json --output ./public/index.html --title 'Nest.js - API - Template'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate": "prisma format && prisma generate && prisma migrate dev --name init",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@nestjs/common": "10.0.5",
    "@nestjs/core": "10.0.5",
    "@nestjs/devtools-integration": "0.1.5",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.0.5",
    "@nestjs/swagger": "7.0.12",
    "@prisma/client": "4.16.2",
    "bcrypt": "5.1.0",
    "helmet": "7.0.0",
    "js-yaml": "4.1.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.1",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.5",
    "@redocly/cli": "1.0.0-beta.129",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.3",
    "@types/passport-jwt": "3.0.8",
    "@types/passport-local": "1.0.35",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "kill-port": "2.0.1",
    "openapi-to-postmanv2": "4.15.0",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "prisma": "4.16.2",
    "rimraf": "5.0.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
